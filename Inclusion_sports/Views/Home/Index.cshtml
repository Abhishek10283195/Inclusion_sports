
@{
	ViewBag.Title = "Index";
	Layout = "~/Views/_MyLayoutPage.cshtml";
}
<style>
	.container .row .col-sm-4 img {
		max-width: 360px;
		height: 280px;
	}
	.tp-banner-container .tp-banner img {
		width: 100%;
	}

	.tp-banner-container {
		background-image: url('../../images/joshua-burdick-unsplash-169-3.jpg');
		height: 650px;
		background-size: cover;
		background-repeat: no-repeat;
	}

	.tp-banner-container .tp-banner .container {
		line-height: 1.6;
		text-align: center;
		color: white;
		font-family: 'montserratregular';
		max-width: 960px;
	}



	#showcase h1 {
		text-align: center;
		font-size: 48px;
		font-family: 'montserratregular';
		color: white;
		font-size: 50px;
		line-height: 1.3;
		position: relative;
		animation: heading;
		animation-duration: 2s;
		animation-fill-mode: forwards;
	}
	@@keyframes heading{
	  0% {top: -50px;}
	  100% {top: 200px;}
	}
	#content {
		position: relative;
		animation-name: content;
		animation-duration: 2s;
		animation-fill-mode: forwards;
	}
	@@keyframes content {
		0% {
			top: -50px;
		}

		100% {
			top: 200px;
		}
	}
</style>
<!--======= HOME MAIN SLIDER =========-->
<section class="home-slider">
	<div class="tp-banner-container">
		<div class="tp-banner">
			<ul>
				<!-- Slider 1 -->
				<li>
					@*data-transition="fade" data-slotamount="5" data-masterspeed="700">
		<img src="~/images/joshua-burdick-unsplash-169-3.jpg" alt="" data-bgfit="cover" data-bgrepeat="no-repeat">*@

					<header id="showcase">
						<h1>INCLUSION THROUGH SPORTS</h1>
					</header>
					<div id="content" class="container">
						Promoting inclusive sports across all diversities
					</div>

				</li>
			</ul>
		</div>
	</div>
</section>

<!--======= ABOUT SECTION =========-->
<section class="sectoion-100px about">
	<div class="container">
		<!-- Tittle -->
		<div class="tittle">
			<h4>About the Website</h4>
			<p>Find your place in Sports, Meet people, Build connections, Know your rights</p>
		</div>
		<ul class="row">
			<li class="col-sm-4">
				<img class="img-responsive" src="~/images/cynthia-magana-mFPJXEQfb9M-unsplash.jpg" alt="">
				<h6 class="margin-top-20">Who we are</h6>
				<p>
					Sports entusiasts from Monash University, promoting the inclusion through sports across all diversities
				</p>
			</li>
			<li class="col-sm-4">
				<img class="img-responsive" src="~/images/about-img-2.jpg" alt="">
				<h6 class="margin-top-20">our mission</h6>
				<p>
					Promote LGBT community to better engage in Sports by building connections and recognitions
				</p>
			</li>
			<li class="col-sm-4">
				<img class="img-responsive" src="~/images/jed-villejo-bEcC0nyIp2g-unsplash.jpg" alt="">
				<h6 class="margin-top-20">our vision</h6>
				<p>
					Educate the rights of LGBT to the society, Increased sports participation from LGBT communitites
				</p>
			</li>
		</ul>
	</div>
</section>

