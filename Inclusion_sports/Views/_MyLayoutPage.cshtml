<!DOCTYPE html>
<style>

	.container-fuild img {
		height: 56px;
	}

	.ownmenu .my-class {
	font-size: 16px;
	}

	.logo h3 {
	margin-left: 20px;
	}
	.main-header {
	padding-bottom: 0px;
	}
</style>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	<link href="~/css/animate.css" rel="stylesheet" />
	<link href="~/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/css/font-awesome.min.css" rel="stylesheet" />
	<link href="~/css/main.css" rel="stylesheet" />
	<link href="~/css/responsive.css" rel="stylesheet" />
	<link href="~/css/style.css" rel="stylesheet" />
	@*@Styles.Render("~/Content/css")*@
	<link href="~/css/Footer.css" rel="stylesheet" />
	@* This is for the mapbox map. *@
	<script src='https://api.mapbox.com/mapbox-gl-js/v0.47.0/mapbox-gl.js'></script>
	<link href='https://api.mapbox.com/mapbox-gl-js/v0.47.0/mapbox-gl.css' rel='stylesheet' />
	@*This is used for the Geocoding Mapbox API*@
	<script src='https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v2.2.0/mapbox-glgeocoder.min.js'></script>
	<link rel='stylesheet' href='https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-glgeocoder/v2.2.0/mapbox-gl-geocoder.css' type='text/css' />
	<script src="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v5.0.0/mapbox-gl-geocoder.min.js"></script>
	<link rel="stylesheet" href="https://api.mapbox.com/mapbox-gl-js/plugins/mapbox-gl-geocoder/v5.0.0/mapbox-gl-geocoder.css" type="text/css">
	<a href="~/fonts/fontawesome-webfont.eot"></a>
	<a href="~/fonts/fontawesome-webfont.ttf"></a>
	<a href="~/fonts/fontawesome-webfont.woff"></a>
	<a href="~/fonts/fontawesome-webfont.woff2"></a>
	<a href="~/fonts/FontAwesome.otf"></a>
	<a href="~/fonts/glyphicons-halflings-regular.eot"></a>
	<a href="~/fonts/glyphicons-halflings-regular.ttf"></a>
	<a href="~/fonts/glyphicons-halflings-regular.woff"></a>
	<a href="~/fonts/glyphicons-halflings-regular.woff2"></a>
	<a href="~/fonts/montserrat-bold-webfont.eot"></a>
	<a href="~/fonts/montserrat-bold-webfont.ttf"></a>
	<a href="~/fonts/montserrat-bold-webfont.woff"></a>
	<a href="~/fonts/montserrat-bold-webfont.woff2"></a>
	<a href="~/fonts/montserrat-hairline-webfont.eot"></a>
	<a href="~/fonts/montserrat-hairline-webfont.ttf"></a>
	<a href="~/fonts/montserrat-hairline-webfont.woff"></a>
	<a href="~/fonts/montserrat-hairline-webfont.woff2"></a>
	<a href="~/fonts/montserrat-light-webfont.eot"></a>
	<a href="~/fonts/montserrat-light-webfont.ttf"></a>
	<a href="~/fonts/montserrat-light-webfont.woff"></a>
	<a href="~/fonts/montserrat-light-webfont.woff2"></a>
	<a href="~/fonts/montserrat-regular-webfont.eot"></a>
	<a href="~/fonts/montserrat-regular-webfont.ttf"></a>
	<a href="~/fonts/montserrat-regular-webfont.woff"></a>
	<a href="~/fonts/montserrat-regular-webfont.woff2"></a>
	<link href="~/fonts/montserrat-fonts.css" rel="stylesheet" />
	<script src="~/js/bootstrap.min.js"></script>
	<script src="~/js/counter.js"></script>
	<script src="~/js/jquery-1.11.3.min.js"></script>
	<script src="~/js/jquery.isotope.min.js"></script>
	<script src="~/js/main.js"></script>
	<script src="~/js/owl.carousel.min.js"></script>
	<script src="~/js/own-menu.js"></script>
</head>
<body>
	<div id="wrap">
		<div id="main-wrapper">
			<header class="main-header">
				<div class="container-fuild">
					<div class="logo"> <a href="#."> <img src="~/images/project_logo.png" alt="Inclusion through Sports"></a> </div>
					<!-- Nav -->
					<nav>
						<ul id="ownmenu" class="ownmenu">
							<li>@Html.ActionLink("Home", "Index", "Home")</li>
							<li>@Html.ActionLink("Find Clubs", "Index", "Maps")</li>
							<li>@Html.ActionLink("Events", "Index", "Events")</li>
						</ul>
					</nav>
				</div>
			</header>
		</div>
	</div>
	<div>
		@RenderBody()
	</div>
	<footer class="footer-section">
		<div class="container">
			<div class="footer-content pt-5 pb-5">
				<div class="row">
					<div class="col-lg-3 col-md-6">
						<div class="footer-widget">
							<div class="footer-logo">
								<a href="Home/Index"><img src="~/images/project_logo.png" class="img-fluid" alt="logo"></a>
							</div>
							<div class="footer-text">
								<h3>PRIDE MELBSPORTS.</h3>
							</div>
						</div>
					</div>
					<div class="col-lg-3 col-md-6">
						<div class="footer-widget">
							<div class="footer-widget-heading">
								<h3>Useful Links</h3>
							</div>
							<ul>
								<li><a href="Home/Index">Home</a></li>
								<li><a href="#services">Services</a></li>
								<li><a href="#about">About Us</a></li>
								<li><a href="Maps/Index">Find Clubs</a></li>
								<li><a href="Events/Index">Event Calendar</a></li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
	</footer>
	<script src="~/js/jquery-1.11.3.min.js"></script>
	<script src="~/js/bootstrap.min.js"></script>
	@RenderSection("Scripts", false)
</body>
</html>
